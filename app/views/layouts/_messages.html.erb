<div class="container" >
  <% flash.each do |name,msg| %>
    <div class="alert alert-success" role="alert",id="flash_message">
      <%= msg %>
      <button type="button" class="close" data-dismiss="alert" aria-label="Close">
        <span aria-hidden="true">&times;</span>
      </button>
    </div>
  <%end%>
</div>
